{
  "name": "oraclecloud",
  "version": "1.0.0",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start_common": "nodemon --config app/common_routes/nodemon.json",
    "start_scraper": "nodemon --config app/scraper/nodemon.json",
    "start_shifts": "nodemon --config app/shifts/nodemon.json",
    "build": "tsc -p tsconfig.build.json ; node fix-imports.js",
    "start": "pm2 delete all && pm2 start ecosystem.config.cjs",
    "stop": "pm2 stop ecosystem.config.cjs",
    "restart": "pm2 restart ecosystem.config.cjs",
    "logs": "pm2 logs",
    "monit": "pm2 monit",
    "status": "  pm2 status"
  },
